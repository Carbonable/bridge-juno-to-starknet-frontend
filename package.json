{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "deploy": "fly deploy --remote-only",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "lint": "eslint app --ext .ts,.tsx",
    "lint:fix": "eslint app --fix --ext .ts,.tsx",
    "start": "remix-serve build",
    "typecheck": "tsc -b",
    "prepare": "husky install"
  },
  "dependencies": {
    "@cosmjs/cosmwasm-stargate": "0.27",
    "@cosmjs/crypto": "0.27",
    "@cosmjs/stargate": "0.27",
    "@remix-run/node": "^1.8.2",
    "@remix-run/react": "^1.8.2",
    "@remix-run/serve": "^1.8.2",
    "@starknet-react/core": "^0.14.8",
    "classnames": "^2.3.2",
    "isbot": "^3.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "starknet": "^4.9.0",
    "tailwindcss-radix": "^2.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@remix-run/dev": "^1.8.2",
    "@remix-run/eslint-config": "^1.8.2",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.27.0",
    "eslint-plugin-react": "^7.31.11",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "tailwindcss": "^3.2.4",
    "turbo": "^1.6.3",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --quiet --fix --cache"
  }
}
